#

declare -A lb_values=(
    [ssh_user]='whoami'
    [uname]='uname -a'
    [os]='cat /etc/os-release | grep -e "^ID=" | cut -d"=" -f2'
    [hostname]='cat /etc/hostname'
    [inodes_use_max]='df -l --output=ipcent 2>/dev/null | grep -v I | cut -d"%" -f1 | sort -u | tail -1 | xargs'
    [mem_available]='cat /proc/meminfo | grep MemAvailable | cut -d":" -f2 | rev | cut -d" " -f2 | rev'
    [php_tz]="php -r \"echo date('P');\""
    [php_version]="php -v | head -1 | cut -d' ' -f2"
)
